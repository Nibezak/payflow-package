{
    "name": "payflowphp/core",
    "description": "Payflow Laravel e-commerce core functionality.",
    "keywords": [
        "payflow",
        "laravel",
        "ecommerce",
        "e-commerce",
        "headless",
        "store",
        "shop",
        "cart"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Payflow",
            "homepage": "https://payflowphp.io/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Payflow\\": "src",
            "Payflow\\Database\\Factories\\": "database/factories",
            "Payflow\\Database\\Seeders\\": "database/seeders",
            "Payflow\\Database\\State\\": "database/state"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Payflow\\PayflowServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-exif": "*",
        "ext-intl": "*",
        "laravel/framework": "^10.0|^11.0",
        "spatie/laravel-medialibrary": "^11.0.0",
        "spatie/laravel-activitylog": "^4.4",
        "laravel/scout": "^10.0",
        "cartalyst/converter": "^8.0|^9.0",
        "kalnoy/nestedset": "^v6.x-dev",
        "doctrine/dbal": "^3.6",
        "lukascivil/treewalker": "0.9.1",
        "spatie/php-structure-discoverer": "^2.0",
        "spatie/laravel-blink": "^1.7"
    }
}