{
  "name": "payflowphp/opayo",
  "type": "project",
  "description": "Opayo payment driver for Payflow.",
  "keywords": [
    "payflowphp",
    "laravel",
    "ecommerce",
    "e-commerce",
    "headless",
    "store",
    "shop",
    "cart",
    "opayo"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Payflow",
      "homepage": "https://payflowphp.io/"
    }
  ],
  "require": {
    "php": "^8.2",
    "payflowphp/core": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Payflow\\Opayo\\": "src/"
    }
  },
  "extra": {
    "payflow": {
      "name": "Opayo Payments"
    },
    "laravel": {
      "providers": [
        "Payflow\\Opayo\\OpayoServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}