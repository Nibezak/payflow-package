{
  "name": "payflowphp/stripe",
  "type": "project",
  "description": "Stripe payment driver for Payflow.",
  "keywords": [
    "payflowphp",
    "laravel",
    "ecommerce",
    "e-commerce",
    "headless",
    "store",
    "shop",
    "cart",
    "stripe"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Payflow",
      "homepage": "https://payflowphp.io/"
    }
  ],
  "require": {
    "php": "^8.2",
    "payflowphp/core": "self.version",
    "stripe/stripe-php": "^14.4"
  },
  "autoload": {
    "psr-4": {
      "Payflow\\Stripe\\": "src/"
    }
  },
  "extra": {
    "payflow": {
      "name": "Stripe Payments"
    },
    "laravel": {
      "providers": [
        "Payflow\\Stripe\\StripePaymentsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}